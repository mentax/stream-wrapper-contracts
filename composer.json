{
	"name": "orisai/stream-wrapper-contracts",
	"description": "Interface for PHP stream wrapper methods",
	"keywords": [
		"abstractions",
		"contracts",
		"decoupling",
		"interfaces",
		"interoperability",
		"orisai",
		"standards",
		"stream",
		"wrapper"
	],
	"type": "library",
	"license": "MPL-2.0",
	"homepage": "https://github.com/orisai/stream-wrapper-contracts",
	"authors": [
		{
		  "name": "Marek Barto≈°",
		  "homepage": "https://github.com/mabar"
		}
	],
	"require": {
		"php": "7.4 - 8.4"
	},
	"require-dev": {
		"brianium/paratest": "^6.3.0",
		"infection/infection": "^0.26.0|^0.27.0|^0.28.0|^0.29.0",
		"orisai/coding-standard": "^3.0.0",
		"phpstan/extension-installer": "^1.0.0",
		"phpstan/phpstan": "^1.0.0",
		"phpstan/phpstan-deprecation-rules": "^1.0.0",
		"phpstan/phpstan-phpunit": "^1.0.0",
		"phpstan/phpstan-strict-rules": "^1.0.0",
		"phpunit/phpunit": "^9.5.0",
		"staabm/annotate-pull-request-from-checkstyle": "^1.7.0"
	},
	"autoload": {
		"psr-4": {
			"Orisai\\StreamWrapperContracts\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Orisai\\StreamWrapperContracts\\": "tests"
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"config": {
		"sort-packages": true,
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true,
			"infection/extension-installer": true,
			"phpstan/extension-installer": true
		}
	}
}
